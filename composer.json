{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^8.1",
    "ext-pdo": "*",
    "ext-pdo_pgsql": "*",
    "symfony/console": "^6.4",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "^6.4",
    "symfony/runtime": "^6.4",
    "symfony/yaml": "^6.4",
    "doctrine/orm": "^2.18",
    "doctrine/doctrine-bundle": "^2.11",
    "symfony/uid": "^6.4",
    "doctrine/annotations": "*",
    "symfony/serializer": "*",
    "symfony/property-access": "*",
    "symfony/validator": "^7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "^7.4",
    "symfony/css-selector": "^7.4",
    "symfony/phpunit-bridge": "^6.4",
    "doctrine/doctrine-migrations-bundle": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "auto-scripts": [],
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  }
}
